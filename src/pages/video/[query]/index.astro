// src/pages/cari/[query]/index.astro
// Ini akan mengalihkan /cari/keyword ke /cari/keyword/1

import { Astro } from 'astro';

const { query } = Astro.params;

// Alihkan ke halaman pertama paginasi
// Ini akan menghasilkan HTTP 302 redirect
return Astro.redirect(`/video/${query}/1`);
